{
   "name": "TinyPng Helper",
   "description": "Auto download images in tinypng.com",
   "icons": { "128": "icon_128.png" },
   "version": "1.0",
   "permissions": [
      "activeTab"
    ],
   "background": {
     "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
    "default_icon": "icon.png",
      "default_title": "touch to download"
    },
    "manifest_version": 2
}